# Production environment variables
# These are baked into the build at compile time

# API URL for production
VITE_API_URL=https://citationformatchecker.com/api

# Enable gated results feature in production
VITE_GATED_RESULTS_ENABLED=true

# Mock mode should be disabled in production
VITE_MOCK_MODE=false

# Polar checkout integration
VITE_POLAR_ACCESS_TOKEN=YOUR_POLAR_ACCESS_TOKEN_HERE
